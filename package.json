{
  "name": "five-films",
  "version": "0.0.0",
  "description": "Hello Expo!",
  "author": null,
  "license": "",
  "main": "bootstrap.js",
  "scripts": {
    "test": "node ./node_modules/jest/bin/jest.js --watch",
    "init": "npm run link-local && npm run link-install",
    "link-local": "npm run link-interfaces && npm run link-services",
    "link-interfaces": "cd src/interfaces && npm link",
    "link-services": "cd src/services && npm link",
    "link-install": "npm link @five-films/interfaces @five-films/services"
  },
  "jest": {
    "preset": "jest-expo-ts",
    "setupFiles": [
      "./jest-setup.js"
    ],
    "transformIgnorePatterns": [
      "!node_modules/@five-films/"
    ]
  },
  "dependencies": {
    "expo": "^20.0.0",
    "inversify": "^4.3.0",
    "merge": "^1.2.0",
    "react": "16.0.0-alpha.12",
    "react-native": "https://github.com/expo/react-native/archive/sdk-20.0.0.tar.gz",
    "react-native-modular-bootstrapper": "^1.0.2",
    "react-navigation": "^1.0.0-beta.13",
    "reflect-metadata": "^0.1.10"
  },
  "devDependencies": {
    "@types/jest": "^20.0.8",
    "@types/react": "^16.0.5",
    "@types/react-native": "^0.48.0",
    "@types/react-navigation": "^1.0.21",
    "@types/react-test-renderer": "^15.5.4",
    "babel-plugin-root-import": "^5.1.0",
    "jest-expo-ts": "^21.0.0",
    "react-native-typescript-transformer": "^1.1.3",
    "typescript": "^2.5.2",
    "xhr2": "^0.1.4"
  }
}
